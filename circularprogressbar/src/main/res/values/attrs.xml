<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircularProgressBar">
        <attr name="cpb_state" format="enum" >
            <enum name="loading" value="1"/>
            <enum name="progress_direct" value="2"/>
            <enum name="progress_invert" value="3"/>
        </attr>
        <attr name="cpb_progress_value" format="integer" />
        <attr name="cpb_progress_color" format="color" />
        <attr name="cpb_progress_width" format="dimension" />
        <attr name="cpb_background_color" format="color" />
        <attr name="cpb_background_width" format="dimension" />
    </declare-styleable>
</resources>